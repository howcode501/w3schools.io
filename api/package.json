{
  "name": "uls-api",
  "version": "1.0.0",
  "main": "src/main.js",
  "license": "MIT",
  "scripts": {
    "prisma-generate": "DB_URI=$ULS_API_POSTGRES_URI npx prismerge -i prismerge.json && npx prisma generate",
    "migrate-deploy": "DB_URI=$ULS_API_POSTGRES_URI npx prisma migrate deploy",
    "start:production": "DB_URI=$ULS_API_POSTGRES_URI node ./src/main.js",
    "start": "DB_URI=$ULS_API_POSTGRES_URI nodemon src/main.js | bunyan",
    "start-win": "nodemon src/main.js | bunyan",
    "job": "DB_URI=$ULS_API_POSTGRES_URI node src/jobs/index.js",
    "test": "NODE_ENV=test mocha --exit \"./test/**/*.spec.js\"",
    "dbtest:initialize": "NODE_ENV=test ./scripts/database.sh initialize",
    "dbtest:drop": "NODE_ENV=test ./scripts/database.sh drop",
    "dbtest:migrate": "NODE_ENV=test ./scripts/database.sh migrate",
    "dbtest:seed": "NODE_ENV=test ./scripts/database.sh seed",
    "dbtest:interactive": "NODE_ENV=test ./scripts/database.sh interactive",
    "db:initialize": "./scripts/database.sh initialize",
    "db:migrate": "./scripts/database.sh migrate",
    "db:migrate-create": "./scripts/database.sh migrate-create",
    "db:push": "./scripts/database.sh push",
    "db:seed": "./scripts/database.sh seed",
    "db:interactive": "./scripts/database.sh interactive",
    "db:reset": "./scripts/database.sh reset",
    "maxmind:update": "node ./node_modules/geoip-lite/scripts/updatedb.js license_key=${MAXMIND_LICENSE_KEY}"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "doc/*"
    ],
    "ext": "js,json,yaml"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.350.0",
    "@aws-sdk/s3-presigned-post": "^3.353.0",
    "@mailchimp/mailchimp_marketing": "^3.0.80",
    "@node-saml/passport-saml": "^4.0.1",
    "@notifire/core": "^0.3.6-alpha.0",
    "@notifire/nodemailer": "^0.3.6-alpha.0",
    "@notifire/sendgrid": "^0.3.6-alpha.0",
    "@notifire/ses": "^0.3.6-alpha.0",
    "@prisma/client": "^4.7.1",
    "@quixo3/prisma-session-store": "^3.1.10",
    "@socket.io/redis-adapter": "^7.2.0",
    "axios": "^1.2.1",
    "bcrypt": "^5.0.1",
    "bunyan": "^1.8.15",
    "commander": "^9.4.1",
    "compose-middleware": "^5.0.1",
    "connect-redis": "^6.1.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-fileupload": "^1.4.0",
    "express-jwt-permissions": "^1.3.7",
    "express-session": "^1.17.3",
    "express-useragent": "^1.0.15",
    "generate-password": "^1.7.0",
    "glob": "^8.0.3",
    "handlebars": "^4.7.7",
    "http": "^0.0.1-security",
    "http-errors": "^2.0.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.1",
    "jwt-decode": "^3.1.2",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "nanoid": "^3.1.31",
    "nanoid-dictionary": "^4.3.0",
    "node-cron": "^3.0.2",
    "node-forge": "^1.3.1",
    "node-jsonwebtoken": "^0.0.1",
    "node-redis": "^0.1.7",
    "node-redis-retry-strategy": "^2.1.1",
    "nodejs-base64": "^2.0.0",
    "nodejs-health-checker": "^1.6.2",
    "nodemon": "^2.0.20",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-strategy": "^1.0.0",
    "redis": "^4.5.1",
    "socket.io": "^4.5.4",
    "socket.io-client": "^4.5.4",
    "stripe": "^12.6.0",
    "uuid": "^9.0.0",
    "yaml": "^2.3.0-5",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.18",
    "@babel/eslint-parser": "^7.16.0",
    "@faker-js/faker": "^7.2.0",
    "@prisma-utils/prismerge": "^1.3.2",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "eslint": ">=5.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-next": "^13.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-mocha": "^10.0.3",
    "mocha": "^10.0.0",
    "mocha-logger": "^1.0.7",
    "nx": "^15.2.1",
    "prettier": "2.7.1",
    "prisma": "^4.7.1"
  },
  "overrides": {
    "node-fetch": "^2.6.7"
  }
}
